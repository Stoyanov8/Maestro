@model IEnumerable<RequestOutputModel>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Description</th>
            <th scope="col">Category</th>
            <th scope="col">Is Completed</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@m.Description</td>
                <td>@m.CategoryName</td>
                <td>@(m.IsActive ? "-" : "+")</td>
            </tr>
        }
    </tbody>
</table>