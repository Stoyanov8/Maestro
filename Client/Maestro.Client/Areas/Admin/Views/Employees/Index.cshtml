@model IEnumerable<EmployeeInformationOutputModel>


<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Current tasks</th>
            <th scope="col">Employee since</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@m.FirstName</td>
                <td>@m.LastName</td>
                <td>@m.CurrentWorkCount</td>
                <td>@m.EmployeeSince</td>
            </tr>
        }
    </tbody>
</table>